---
import Header from "./Header.astro";
import Footer from "./Footer.astro";
import "./global.css";
const { title = "Sound of Simone" } = Astro.props;
---

<!DOCTYPE html>
<html lang="no">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>{title}</title>
  </head>
  <body>
    <Header />
    <main>
      <slot />
    </main>
    <Footer />
  </body>
</html>
